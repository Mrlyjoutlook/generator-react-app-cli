{
  "name": "react",
  "version": "0.5.3",
  "description": "MRLYJ - React Mobx Starter Kit",
  "main": "index.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "compile": "better-npm-run compile",
    "dll": "better-npm-run dll",
    "mock": "better-npm-run mock",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "webpack-defaults": "webpack-defaults"
  },
  "betterScripts": {
    "dll": {
      "command": "node bin/dll",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command":
        "nodemon bin/dev --ignore bin --ignore build --ignore dll --ignore mock --ignore public --ignore src --ignore node_modules",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development"
      }
    },
    "compile": {
      "command": "node bin/build",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "mock": {
      "command": "cd mock && node mockServer",
      "env": {}
    },
    "deploy": {
      "command": "npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    }
  },
  "author": "mrlyj",
  "license": "MIT",
